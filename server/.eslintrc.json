{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": ["plugin:node/recommended", "eslint:recommended"],
  "parser": "babel-eslint",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["import"],
  "rules": {
    "node/no-unsupported-features/es-syntax": 0
  }
}
